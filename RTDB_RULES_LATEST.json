{
  "rules": {
    "qrcodes": {
      "$qrToken": {
        ".read": "auth != null",
        ".write": false
      }
    },
    "leaderboard": {
      "top10": {
        ".read": "auth != null",
        ".write": false
      }
    },
    "ranks": {
      "$uid": {
        ".read": "auth != null",
        ".write": false
      }
    },
    "adminCache": {
      "emails": {
        "$email": {
          ".read": "auth != null && root.child('admins').child(auth.uid).exists()",
          ".write": false
        }
      },
      "participantIds": {
        "$participantId": {
          ".read": "auth != null && root.child('admins').child(auth.uid).exists()",
          ".write": false
        }
      },
      "participants": {
        ".read": "auth != null && root.child('admins').child(auth.uid).exists()",
        ".write": false
      }
    },
    ".read": false,
    ".write": false
  }
}

